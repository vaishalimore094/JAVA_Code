package Factorial_Number;

public class Factorial_number {

	public static void main(String[] args) {
		{
			
//			3!
//			3*2*1=
			
			int num=3;
			int fact=1;//3
			
			for (int i=num;i>=1;i--)//
			{
				fact=fact*i; //
				
			}
			
			System.out.println("factorial of number is "+fact);
			
			

		}

	


	}

}
